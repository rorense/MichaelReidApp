{
  "expo": {
    "name": "MICHAEL REID GALLERY",
    "slug": "app",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/image/app_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "package": "com.gherkinmedia.michaelreid",
      "bundleIdentifier": "com.gherkinmedia.michaelreid",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "This app requires access to your photo library to allow you to select and upload images for your artwork collection. For example, you can choose images from your library to add to your personal gallery of artwork."
      }
    },
    "config": {
      "usesPushNotifications": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/image/Logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gherkinmedia.michaelreid"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/image/app_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-updates",
        {
          "username": "dryguy112"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9525aa74-00d1-45cd-9667-5a22f48202a8"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9525aa74-00d1-45cd-9667-5a22f48202a8"
    }
  },
  "extra": {
    "eas": {
      "projectId": "9525aa74-00d1-45cd-9667-5a22f48202a8"
    }
  },
  "plugins": [
    "expo-router"
  ]
}
