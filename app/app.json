{
  "expo": {
    "name": "Michael Reid",
    "slug": "app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/image/Logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "package": "com.gherkinmedia.michaelreid",
      "bundleIdentifier": "com.gherkinmedia.michaelreid",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesPushNotifications": false
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/image/Logo.png",
          "backgroundColor": "#ffffff"
        },
        "package": "com.gherkinmedia.michaelreid"
      },
      "web": {
        "bundler": "metro",
        "output": "static",
        "favicon": "./assets/images/favicon.png"
      },
      "plugins": [
        "expo-router",
        [
          "expo-splash-screen",
          {
            "image": "./assets/image/Logo.png",
            "imageWidth": 200,
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
          }
        ]
      ],
      "experiments": {
        "typedRoutes": true
      },
      "extra": {
        "router": {
          "origin": false
        },
        "eas": {
          "projectId": "9525aa74-00d1-45cd-9667-5a22f48202a8"
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "9525aa74-00d1-45cd-9667-5a22f48202a8"
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
